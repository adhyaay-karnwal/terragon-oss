{
  "name": "@rover/docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "fumadocs-mdx && next build",
    "dev": "next dev --turbo --port 3001",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "tsc-check": "tsc --noEmit",
    "tsc-watch": "tsc --noEmit --watch --preserveWatchOutput",
    "format": "prettier --write . --ignore-path ../../.prettierignore --config ../../.prettierrc",
    "format-check": "prettier --check . --ignore-path ../../.prettierignore --config ../../.prettierrc"
  },
  "dependencies": {
    "fumadocs-core": "15.5.3",
    "fumadocs-mdx": "11.6.9",
    "fumadocs-ui": "15.5.3",
    "lucide-react": "^0.511.0",
    "next": "15.4.8",
    "react": "19.1.2",
    "react-dom": "19.1.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/mdx": "^2.0.13",
    "@types/node": "24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3"
  }
}
